import sys
import argparse


def fib (num):
    if num == 0:
        a = 1
    elif num == 1:
        a = 1
    else:
        a = fib(num-1) + fib(num-2)
    return a


def createParser ():
    parser = argparse.ArgumentParser()
    parser.add_argument ('num', type = int)

    return parser


if __name__ == '__main__':
    parser = createParser()
    namespace = parser.parse_args()
    print (fib(namespace.num))
